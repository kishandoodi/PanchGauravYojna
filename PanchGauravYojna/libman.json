{
  "version": "3.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery-validation@latest",
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "library": "jquery-validation-unobtrusive@latest",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.js.map",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.min.js.map"
      ]
    },
    {
      "library": "jquery-toast-plugin@1.3.2",
      "destination": "wwwroot/lib/jquery-toast-plugin/"
    },
    //{
    //  "library": "font-awesome@4.7.0",
    //  "destination": "wwwroot/lib/font-awesome/"
    //},
    {
      "library": "datatables.net@2.3.2",
      "destination": "wwwroot/lib/datatables.net/"
    },
    {
      "library": "font-awesome@latest",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "sweetalert2@latest",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "library": "toastr@2.1.4",
      "destination": "wwwroot/toastr/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "js/bootstrap-datepicker.js",
        "dist/css/bootstrap-datepicker.css",
        "dist/css/bootstrap-datepicker.min.css",
        "dist/js/bootstrap-datepicker.js",
        "dist/js/bootstrap-datepicker.min.js",
        "dist/css/bootstrap-datepicker3.css.map"
      ]
    }
  ]
}