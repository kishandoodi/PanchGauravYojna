	<!-- To customize the asp.net core module uncomment and edit the following section. 
 For more info see https://go.microsoft.com/fwlink/?linkid=838655 -->
   
	<system.webServer>
		<httpErrors errorMode="Custom" existingResponse="Replace">
			<remove statusCode="403" />
			<remove statusCode="404" />
			<error statusCode="403" path="/SDGV2/StatusError/403" responseMode="ExecuteURL" />
			<error statusCode="404" path="/SDGV2/StatusError/404" responseMode="ExecuteURL" />
		</httpErrors>

		<security>
			<requestFiltering removeServerHeader="true">
				<requestLimits maxAllowedContentLength="524288000" /> 
			</requestFiltering>
			<!--<requestFiltering removeServerHeader="true"></requestFiltering> 500MB-->
		</security>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By"/>
				<remove name="Server" />
				<!--<add name="Content-Security-Policy" value="default-src*;'self'; 'unsafe-inline' 'unsafe-eval';" />-->
			</customHeaders>
		</httpProtocol>
	</system.webServer>

</configuration>