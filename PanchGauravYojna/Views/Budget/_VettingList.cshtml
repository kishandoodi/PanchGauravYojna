@model List<MO.BudgetMaster.VettingList>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Gatividhi</th>
            <th>GatividhiName</th>
            <th>Budget</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            int i = 1;
            foreach (var item in Model)
            {
                <tr>
                    <td>@i</td>
                    <td>@item.Gatividhi</td>
                    <td>@item.GatividhiName</td>
                    <td>@item.Budget</td>
                </tr>
                i++;
            }
        }
        else
        {
            <tr>
                <td colspan="4">No record found</td>
            </tr>
        }
    </tbody>
</table>
