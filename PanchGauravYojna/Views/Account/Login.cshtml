<script nonce="@nonce">
        document.addEventListener("DOMContentLoaded", function () {
        const passwordInput = document.getElementById("Password");
        const nonceInput = document.getElementById("nonce");
        const hashedInput = document.getElementById("HashedPassword");

        async function updateHash() {
            const password = passwordInput.value;
            const nonce = nonceInput.value;
            const salted = password + nonce;

            const hash = await hashPassword(salted);
            hashedInput.value = hash;
            console.log("Hashed password:", hash);
        }

        // Event listeners
        passwordInput.addEventListener("keyup", updateHash);
        passwordInput.addEventListener("input", updateHash);
    });

    async function hashPassword(text) {
        const hash = CryptoJS.SHA256(text).toString(CryptoJS.enc.Hex);
        console.log("Client Hash:", hash);
        return hash;
    }

</script>
