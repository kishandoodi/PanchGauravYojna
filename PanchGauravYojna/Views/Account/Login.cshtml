@model MO.Login.loginModels.loginCredentials
@{
    ViewData["Title"] = "Login";
    Layout = "~/Views/Shared/_LayoutOut.cshtml";
}
@{
    var returnUrl = Convert.ToString(Context.Request.Query["ReturnUrl"]);
    var nonce = Context.Items["CSPNonce"]?.ToString();
}
<style nonce="@nonce">
    .margin_top {
        margin-top: 26%;
        margin-bottom: 1%;
    }

    .bg_color {
        background: rgb(3,40,92);
        background: linear-gradient(0deg, rgba(3,40,92,1) 0%, rgba(191,29,62,1) 100%);
        margin: auto !important;
    }

    .bg_card {
        color: #fff;
        border: 0px;
        margin-top: -230px !important;
    }

    .color-white {
        color: #fff;
    }
</style>

<div class="container margin_top">
    <div class="col-12 col-xl-5 col-xxl-4 auth-cover-right align-items-center justify-content-center bg_color">
        <div class="card rounded-0 shadow-none bg-transparent mb-0 bg_card">
            <div class="card-body p-sm-5">
                <div class="">
                    <div class="mb-3 text-center">
                        <img src="~/img/sdglogo.png" width="60" alt="">
                    </div>
                    <div class="text-center mb-4">
                        <h5 class="color-white" style="">Panch Gaurav </h5>
                        <p class="mb-0">Please log in to your account</p>
                    </div>
                    <div class="form-body">
                        @* <input type="hidden" id="nonce" value="@ViewData["loginNonce"]" /> *@
                        @* <form class="row g-3" autocomplete="off"> *@
                        <form class="row g-3" id="myForm" asp-action="Login" asp-controller="Account"
                              enctype="multipart/form-data"
                              data-ajax="true"
                              data-ajax-method="POST"
                              data-ajax-begin="OnBegin"
                              data-ajax-failure="OnFailure"
                              data-ajax-success="OnSuccess"
                              data-ajax-complete="OnComplete"
                              autocomplete="off">

                            @Html.AntiForgeryToken()
                            <input type="hidden" id="nonce" name="nonce" value="@ViewData["loginNonce"]" />
                            <div class="col-12">
                                <label asp-for="Username" class="form-label"></label>
                                <input asp-for="Username" class="form-control alphanumeric_username" placeholder="UserName" maxlength="20" autocomplete="off" />
                                <span asp-validation-for="Username" class="text-danger"></span>
                            </div>

                            <div class="col-12">
                                <label asp-for="Password" class="form-label"></label>
                                <div class="input-group" id="show_hide_password">
                                    <input id="Password" type="password" class="form-control border-end-0" maxlength="20" placeholder="Password" autocomplete="off" />
                                    <input type="hidden" id="HashedPassword" name="HashedPassword" />
                                    <a href="javascript:;" class="input-group-text"><i class="bx bx-hide"></i></a>
                                </div>
                               <span asp-validation-for="Password" class="text-danger"></span>
                                @* <span asp-validation-for="HashedPassword" class="text-danger"></span> *@
                            </div>

                            <div class="col-12">
                                <div class="d-grid">
                                    <button type="submit" class="btn btn-primary" id="btn_login">Sign in</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

@* <script src="~/custom\account.js" asp-append-version="true"></script> *@
<script nonce="@nonce" src="~/dashboardlogin/scripts.b23791aacf819f5925a1.js"></script>
